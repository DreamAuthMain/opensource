{
  "name": "@dreamauth/jwk-resolver",
  "description": "Caching JWK resolver compatible with OIDC Discovery and FAPI JWKS selection.",
  "version": "0.1.0",
  "author": "Chris Ackerman <chris@topher.land>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dreamauthmain/opensource.git"
  },
  "files": [
    "lib/cjs/**",
    "lib/esm/**"
  ],
  "directories": {
    "lib": "lib"
  },
  "type": "module",
  "types": "lib/esm/index.d.ts",
  "module": "lib/esm/index.js",
  "main": "lib/cjs/index.js",
  "exports": {
    ".": {
      "types": "./lib/esm/index.d.ts",
      "import": "./lib/esm/index.js",
      "default": "./lib/cjs/index.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "lint": "eslint .",
    "test": "jest",
    "build": "tsc -b config/tsconfig.build.json && echo '{ \"type\": \"commonjs\" }' > lib/cjs/package.json",
    "prepublishOnly": "npx @dreamauth/prepublish"
  },
  "jest": {
    "preset": "../../config/jest.config.test.cjs"
  },
  "publishConfig": {
    "access": "public"
  }
}
